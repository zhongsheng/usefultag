
<table class="table table-dark">
    <thead>
        <tr>
            <th scope="col">name</th>
            <th scope="col">tag</th>
            <th scope="col">class</th>
            <th scope="col">#</th>

        </tr>
    </thead>
    <tbody>
<% @tags.each do |tag| %>
        <tr>
            <td>
                <%= tag.name %>
            </td>
            <td>
                <%= tag.tag_name %>
            </td>
            <td>
                <%= tag.record_type %>
            </td>
            <td>
                <%= link_to 'Destroy', tag, method: :delete %>
            </td>
        </tr>
<% end %>
    </tbody>
</table>
